# Production environment variables
# These will be overridden by Revolt.overrides.toml

# MongoDB
MONGODB_URI=mongodb://root:CHANGE_ME@mongodb:27017/revolt?authSource=admin

# Redis
REDIS_URI=redis://redis:6379/

# RabbitMQ
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=rabbituser
RABBITMQ_PASS=rabbitpass

# API URLs (will be overridden by Revolt.overrides.toml)
API_URL=https://atuj.ru:9443/api
WS_URL=wss://atuj.ru:9443/ws

# No email verification in production (for now)
SMTP_HOST=
